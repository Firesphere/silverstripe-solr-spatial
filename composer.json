{
    "name": "firesphere/solr-spatial",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "search",
        "solr",
        "configuration",
        "solarium",
        "spatial",
        "latitude",
        "longitude",
        "geolocation"
    ],
    "description": "Spatial searching with Solr and SilverStripe",
    "require": {
        "php": "^7.1 || ^8",
        "firesphere/solr-search": "^1.1"
    },
    "authors": [
        {
            "name": "Simon `Firesphere` Erkelens",
            "email": "firesphere@casa-laguna.net",
            "homepage": "https://casa-laguna.net",
            "role": "Lead developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "scriptfusion/phpunit-immediate-exception-printer": "^1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
